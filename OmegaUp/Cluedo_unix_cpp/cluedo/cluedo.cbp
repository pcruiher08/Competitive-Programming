<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cluedo" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="test">
				<Option output="libinteractive/run" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="'examples/sample.in'" />
				<MakeCommands>
					<Build command="$make -f $makefile all" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile clean" />
					<AskRebuildNeeded command="$make -q -f $makefile all" />
					<SilentBuild command="$make -f $makefile all &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="all">
				<Option output="libinteractive/run" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<MakeCommands>
					<Build command="$make -f $makefile all" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile clean" />
					<AskRebuildNeeded command="$make -q -f $makefile all" />
					<SilentBuild command="$make -f $makefile all &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Unit filename="examples/sample.in">
			<Option target="test" />
		</Unit>
		<Unit filename="cluedo.cpp">
			<Option target="all" />
			<Option target="test" />
		</Unit>
		<Unit filename="Main.cpp">
			<Option target="all" />
			<Option target="test" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger>
				<remote_debugging target="test">
					<options conn_type="0" serial_baud="115200" ip_address="127.0.0.1" ip_port="8042" additional_cmds_before="shell libinteractive/run --debug --pause 'examples/sample.in' &amp;&#x0A;symbol-file libinteractive/cluedo/cluedo_debug&#x0A;set substitute-path libinteractive/cluedo ." />
				</remote_debugging>
				<remote_debugging target="all">
					<options conn_type="0" serial_baud="115200" ip_address="127.0.0.1" ip_port="8042" additional_cmds_before="shell libinteractive/run --debug 'examples/sample.in' &amp;&#x0A;symbol-file libinteractive/cluedo/cluedo_debug&#x0A;set substitute-path libinteractive/cluedo ." />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>